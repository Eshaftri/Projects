<h1>Memebrs</h1>

<p class="btn--wrapper">
  <a class="btn btn-save" href="members/new">New Member</a>
</p>


<table class="members_table" width="100%" cellspacing="0">
  <thead>
    <tr>
      <th>Title</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Address</th>
      <th>DoB</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
    <tr>
      <td><%= member.title %></td>
      <td><%= member.first_name %></td>
      <td><%= member.last_name %></td>
      <td><%= member.address%></td>
      <td><%= member.dob %></td>
      <td>
        <div class="btn-group">
          <form  class="form-del" action="/members/<%= member.id %>/delete" method="POST">
          <a href="/members/<%=member.id%>/edit"   class="btn btn-edit">Edit</a>
          <a href="#" class="btn btn-view">View</a>
            <button type="submit" class="btn btn-delete">Delete</button>
          </form>
        </div>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
